CREATE TABLE NextEpisode (
    id INTEGER,
    tmdbId INTEGER AS Int,
    name TEXT NOT NULL,
    episodeNumber INTEGER AS Int NOT NULL,
    seasonNumber INTEGER AS Int NOT NULL,
    airDateMillis INTEGER,
    imageUrl TEXT,
    PRIMARY KEY (id)
);

insert:
INSERT INTO NextEpisode(tmdbId, name, episodeNumber, seasonNumber, airDateMillis, imageUrl)
VALUES (?, ?, ?, ?, ?, ?);

lastInsertRowId:
SELECT last_insert_rowid();
