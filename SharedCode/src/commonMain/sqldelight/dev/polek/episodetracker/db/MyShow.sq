CREATE TABLE MyShow (
    id INTEGER,
    imdbId TEXT,
    tmdbId INTEGER,
    tvdbId INTEGER,
    facebookId TEXT,
    instagramId TEXT,
    twitterId TEXT,
    name TEXT NOT NULL,
    overview TEXT NOT NULL,
    year INTEGER,
    PRIMARY KEY (id)
);

insert:
INSERT INTO MyShow(imdbId, tmdbId, tvdbId, facebookId, instagramId, twitterId, name, overview, year)
VALUES (?, ?, ?, ?, ?, ?, ?, ?, ?);

deleteByTmdbId:
DELETE FROM MyShow WHERE tmdbId = ?;

selectAll:
SELECT *
FROM MyShow;

isInMyShows:
SELECT count(*) > 0
FROM MyShow
WHERE tmdbId = ?;
